# Add inputs and outputs from these tool invocations to the build variables 
ELFSIZE += \
vitijumps.elf.size \

ELFCHECK += \
vitijumps.elf.elfcheck \


# All Target
all: vitijumps.elf secondary-outputs

# Tool invocations
vitijumps.elf: $(OBJS) ../src/lscript.ld $(USER_OBJS)
	@echo Building target: $@
	@echo Invoking: MicroBlaze gcc linker
	mb-gcc -Wl,-T -Wl,../src/lscript.ld -L../../xilkernel_bsp_0/microblaze_0/lib -mxl-barrel-shift -mxl-pattern-compare -mcpu=v8.20.a -mxl-soft-mul -o"vitijumps.elf" $(OBJS) $(USER_OBJS) $(LIBS)
	@echo Finished building target: $@
	@echo ' '

vitijumps.elf.size: vitijumps.elf
	@echo Invoking: MicroBlaze Print Size
	mb-size vitijumps.elf  |tee "vitijumps.elf.size"
	@echo Finished building: $@
	@echo ' '

vitijumps.elf.elfcheck: vitijumps.elf
	@echo Invoking: Xilinx ELF Check
	elfcheck vitijumps.elf -hw ../../Nexys3_v004_hw_platform/system.xml -pe microblaze_0  |tee "vitijumps.elf.elfcheck"
	@echo Finished building: $@
	@echo ' '

# Other Targets
clean:
	-$(RM) $(OBJS)$(C_DEPS)$(EXECUTABLES)$(ELFSIZE)$(ELFCHECK)$(S_UPPER_DEPS) vitijumps.elf
	-@echo ' '

